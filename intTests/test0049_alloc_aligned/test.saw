m <- llvm_load_module "test.bc";

let dup_spec = do {
  sp <- crucible_alloc (llvm_type "%struct.syndrome_s");
  s <- crucible_fresh_var "s" (llvm_type "%struct.syndrome_s");
  crucible_points_to sp (crucible_term s);
  crucible_execute_func [sp];
};

crucible_llvm_verify m "dup" [] false dup_spec yices;

